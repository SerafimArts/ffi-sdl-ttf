{
    "name": "serafim/ffi-sdl-ttf",
    "description": "PHP FFI to SDL TTF bindings",
    "type": "library",
    "license": "MIT",
    "support": {
        "issues": "https://github.com/SerafimArts/ffi-sdl-ttf/issues",
        "source": "https://github.com/SerafimArts/ffi-sdl-ttf"
    },
    "keywords": [
        "sdl",
        "ffi",
        "bindings",
        "opengl",
        "graphics",
        "library",
        "ttf",
        "font"
    ],
    "homepage": "https://github.com/SerafimArts/ffi-sdl-ttf",
    "authors": [
        {
            "name": "Kirill Nesmeyanov",
            "email": "nesk@xakep.ru"
        }
    ],
    "require": {
        "php": ">=7.4",
        "ext-ffi": "*",
        "ext-mbstring": "*",
        "serafim/ffi-sdl": "~1.0.3"
    },
    "autoload": {
        "psr-4": {
            "Serafim\\SDL\\TTF\\": "src"
        }
    },
    "require-dev": {
        "serafim/ffi-ide-helper": "^1.0"
    },
    "autoload-dev": {
        "psr-4": {
            "Serafim\\SDL\\TTF\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "source"
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    }
}
